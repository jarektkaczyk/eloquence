{
    "name": "sofa/eloquence",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jarektkaczyk/eloquence/issues",
        "source": "https://github.com/jarektkaczyk/eloquence"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ukazap-iav/eloquence-base"
        }
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "http://softonsofa.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "sofa/eloquence-base": "dev-master",
        "sofa/eloquence-metable": "^5.5|^6.0",
        "sofa/eloquence-mutable": "^5.5|^6.0",
        "sofa/eloquence-mappable": "^5.5|^6.0",
        "sofa/eloquence-validable": "^5.5|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.0",
        "squizlabs/php_codesniffer": "2.3.3",
        "mockery/mockery": "0.9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
