{
    "name": "wargog/eloquence",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/wargog/eloquence/issues",
        "source": "https://github.com/wargog/eloquence"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "http://softonsofa.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "wargog/eloquence-base": "^6.0",
        "wargog/eloquence-metable": ">=6.0",
        "wargog/eloquence-mutable": ">=6.0",
        "wargog/eloquence-mappable": ">=6.0",
        "wagog/eloquence-validable": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "2.3.3",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
    }
}
